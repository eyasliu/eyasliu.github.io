{
  "name": "eyasweb-app",
  "version": "0.0.0",
  "description": "is a startkit with react app",
  "keywords": [
    "startkit",
    "app",
    "react",
    "redux"
  ],
  "scripts": {
    "lint": "gulp lint",
    "clean": "gulp clean",
    "start": "cross-env NODE_ENV=production bash run",
    "build": "cross-env NODE_ENV=production gulp build",
    "server": "cross-env NODE_ENV=production node app/server",
    "dev": "cross-env NODE_ENV=development gulp dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "e2e": "cross-env NODE_ENV=test casperjs --compilers js:babel-register test --concise ./test/e2e",
    "dev:test": "concurrent --kill-others --raw \"npm run dev\" \"npm run test:watch\"",
    "test:watch": "cross-env NODE_ENV=test karma start -- --watch --no-single-run",
    "deploy": "npm run build && npm run start"
  },
  "main": "index.js",
